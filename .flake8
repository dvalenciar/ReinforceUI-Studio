[flake8]
max-line-length = 79
ignore = E203, W503, E501,  D101, D100, D415, ANN101, ANN102, ANN401
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    reinforceui_studio/GUI/training_window.py,
    reinforceui_studio/GUI/ui_styles.py,
    reinforceui_studio/GUI/ui_utils.py,
max-complexity = 10
docstring-convention = google